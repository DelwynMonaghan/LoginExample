<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="login.example.com">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="login.example.com.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SignUPActivity"/>
        <activity
            android:name=".EventsPage"/>
        <activity
            android:name=".EventManagerActivity"/>
        <activity
            android:name=".AddEventActivity"/>
        <activity
            android:name=".EditEventActivity"/>
        <activity
            android:name=".StandManagerActivity"/>
        <activity
            android:name=".AddStandActivity"/>
    </application>
</manifest>